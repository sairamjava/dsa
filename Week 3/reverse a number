import java.util.Scanner;
class Main {
    public static void main(String[] args) {
         Scanner scan = new Scanner(System.in);
        System.out.println("enter the number");
        long num = scan.nextLong();
        long reverse =0;
      while(num>0){
          long ld =num%10;
          reverse = (reverse *10)+ld;
          
          num /= 10;
      }
        System.out.println(reverse);
    }
}
