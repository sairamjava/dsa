import java.util.Scanner;
class Main {
  
  
      
    public static void main(String[] args) {
        
     Scanner scan = new Scanner(System.in);
      System.out.println("enter the array length");
     int n = scan.nextInt();
     int [] arr = new int [n];
      System.out.println("enter the array element");
     for(int i=0;i<n;i++){
         arr[i]=scan.nextInt();
     }
     System.out.println("enter the array element to print");
     int arrayElement = scan.nextInt();
     if(arrayElement <= n){
          System.out.println(arr[arrayElement -1]);
     }else{
          System.out.println("your entered number is out of bound");
     }
    
    }

}
